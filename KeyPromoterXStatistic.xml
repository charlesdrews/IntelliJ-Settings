<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="View Breakpoints...">
        <Item>
          <IdeaActionID>ViewBreakpoints</IdeaActionID>
          <ShortCut>'⇧⌘F8'</ShortCut>
          <Description>View Breakpoints...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Captures">
        <Item>
          <IdeaActionID>ActivateCapturesToolWindow</IdeaActionID>
          <Description>Captures</Description>
        </Item>
      </Statistic>
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'⌘5'</ShortCut>
          <Description>Debug</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'app'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌃D'</ShortCut>
          <Description>Debug 'app'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Logcat">
        <Item>
          <IdeaActionID>ActivateLogcatToolWindow</IdeaActionID>
          <ShortCut>'⌘6'</ShortCut>
          <Description>Logcat</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Package">
        <Item>
          <IdeaActionID>NewDir</IdeaActionID>
          <Description>Package</Description>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'⌥⌘R'</ShortCut>
          <Description>Resume Program</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'app'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃R'</ShortCut>
          <Description>Run 'app'</Description>
          <Count>14</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Execution Point">
        <Item>
          <IdeaActionID>ShowExecutionPoint</IdeaActionID>
          <ShortCut>'⌥F10'</ShortCut>
          <Description>Show Execution Point</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Into">
        <Item>
          <IdeaActionID>StepInto</IdeaActionID>
          <ShortCut>'F7'</ShortCut>
          <Description>Step Into</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Out">
        <Item>
          <IdeaActionID>StepOut</IdeaActionID>
          <ShortCut>'⇧F8'</ShortCut>
          <Description>Step Out</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Over">
        <Item>
          <IdeaActionID>StepOver</IdeaActionID>
          <ShortCut>'F8'</ShortCut>
          <Description>Step Over</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'app'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'app'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>